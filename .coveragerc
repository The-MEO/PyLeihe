[run]
branch = True
source = PyLeihe
concurrency = multiprocessing

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    tests/*
precision = 2

[html]
directory = htmlcov

[xml]
output = coverage.xml
